{
  "name": "mijn-app-frontend",
  "description": "The app client for the *Mijn App - Mijn Portaal* project",
  "licence": "UNLICENSED",
  "private": true,
  "version": "1.1.0",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=development --watch --env.NODE_ENV=development",
    "build:prod": "webpack --env.NODE_ENV=production --optimize-minimize",
    "dev": "webpack-dev-server --hot --inline",
    "release": "standard-version"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "eol-last": 0,
      "import/no-unassigned-import": 0,
      "promise/prefer-await-to-then": 0
    }
  },
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "4.1.1",
    "copy-webpack-plugin": "5.0.3",
    "cssnano": "4.1.10",
    "ejs-loader": "0.3.3",
    "eslint": "6.0.1",
    "eslint-config-google": "0.13.0",
    "eslint-plugin-es6": "1.0.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-node": "9.1.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "lighthouse": "5.1.0",
    "moment-locales-webpack-plugin": "1.1.0",
    "postcss": "7.0.17",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-discard-comments": "4.0.2",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.1",
    "postcss-nesting": "7.0.0",
    "postcss-reporter": "6.0.1",
    "postcss-selector-not": "4.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard-version": "6.0.1",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "text-loader": "0.0.1",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.8.0",
    "workbox-webpack-plugin": "4.3.1",
    "xo": "0.24.0"
  },
  "dependencies": {
    "@polymer/iron-pages": "3.0.1",
    "@polymer/paper-input": "3.0.2",
    "@polymer/polymer": "3.3.0",
    "@polymer/paper-dialog": "3.0.1",
    "@polymer/paper-toast": "3.0.1",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "axios": "0.19.0",
    "moment": "2.29.2",
    "mobile-detect": "1.4.3",
    "normalize.css": "8.0.1",
    "pdfmake": "0.1.59",
    "pwa-helpers": "0.9.1",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.5"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "build",
      "dev"
    ]
  }
}
